<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhangxiaoyu.cms.dao.LinksMapper">

	<insert id="insert">
		insert into cms_links set text=#{text},url=#{url},created=#{created}
	</insert>

	<select id="selects" resultType="com.zhangxiaoyu.cms.domain.Links">
		select * from cms_links 
	</select>
</mapper>